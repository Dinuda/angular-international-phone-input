{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"ngx-int-phone-input-app": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "app",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/ngx-int-phone-input-app",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.app.json",
						"assets": ["src/favicon.ico", "src/assets"],
						"styles": [
							"./node_modules/intl-tel-input/build/css/intlTelInput.min.css",
							"src/styles.css"
						],
						"scripts": [],
						"vendorChunk": true,
						"extractLicenses": false,
						"buildOptimizer": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							]
						}
					},
					"defaultConfiguration": ""
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "ngx-int-phone-input-app:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "ngx-int-phone-input-app:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "ngx-int-phone-input-app:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"karmaConfig": "src/karma.conf.js",
						"styles": [
							"./node_modules/int-phone-input/build/css/intPhoneInput.css",
							"src/styles.css"
						],
						"scripts": [],
						"assets": ["src/favicon.ico", "src/assets"]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"ngx-int-phone-input-app-e2e": {
			"root": "e2e/",
			"projectType": "application",
			"prefix": "",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "ngx-int-phone-input-app:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "ngx-int-phone-input-app:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "e2e/tsconfig.e2e.json",
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"ngx-int-phone-input": {
			"root": "projects/ngx-int-phone-input",
			"sourceRoot": "projects/ngx-int-phone-input/src",
			"projectType": "library",
			"prefix": "ngx",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"tsConfig": "projects/ngx-int-phone-input/tsconfig.lib.json",
						"project": "projects/ngx-int-phone-input/ng-package.json"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/ngx-int-phone-input/src/test.ts",
						"tsConfig": "projects/ngx-int-phone-input/tsconfig.spec.json",
						"karmaConfig": "projects/ngx-int-phone-input/karma.conf.js"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/ngx-int-phone-input/tsconfig.lib.json",
							"projects/ngx-int-phone-input/tsconfig.spec.json"
						],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		}
	},
	"defaultProject": "ngx-int-phone-input-app"
}
